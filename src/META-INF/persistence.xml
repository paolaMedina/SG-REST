<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="SG-RESTPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>Clases.Pedido</class>
    <class>Clases.EstadoPedido</class>
    <class>Clases.Factura</class>
    <class>Clases.ProductoFactura</class>
    <class>Clases.Mesa</class>
    <class>Clases.Pago</class>
    <class>Clases.Producto</class>
    <class>Clases.TipoPago</class>
    <class>Clases.CategoriaProducto</class>
    <class>Clases.Empleado</class>
    <class>Clases.CargoEmpleado</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/SG-REST"/>
      <property name="javax.persistence.jdbc.password" value="postgres"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
    </properties>
  </persistence-unit>
</persistence>
